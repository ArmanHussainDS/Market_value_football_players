    "            team_pattern = re.compile(r'(?<=\\>).+(?=<)')\n",
    "            link_pattern = re.compile(r'/Team.+(?=\" )')\n",
    "\n",
    "            for each in soup.find_all('a', attrs={'class':'team-link'}):\n",
    "\n",
