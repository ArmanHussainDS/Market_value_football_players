    "def get_players(teams_df, whoscore=True, team=False):\n",
    "    \n",
    "    players_detail_df = pd.DataFrame()\n",
    "    \n",
    "    for each_link, each_team in zip(list(teams_df.links), list(teams_df.teams)):  \n",
    "        \n",
    "        if whoscore:\n",
    "            URL = 'https://www.whoscored.com' + each_link\n",
    "        else:\n",
    "            URL = 'https://www.transfermarkt.co.uk' + each_link\n",
    "        \n",
    "        print(URL)\n",
    "        soup = get_soup(URL)\n",
    "        players = extract_teams_or_players(soup, each_team, whoscore, team)\n",
    "        players_detail_df = pd.concat([players_detail_df, players], axis=0, sort=True)\n",
    "        \n",
    "        print(\"Complete \" + each_team)\n",
    "        \n",
    "    return players_detail_df"
   ]
